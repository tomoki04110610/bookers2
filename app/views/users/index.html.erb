<%= render "user_info", user: @user %>

<h1>User</h1>
<h4>image</h4>
<h4>name</h4>
<% @users.each do |user| %>
  <% if user.profile_image.attached? %>
    <%= image_tag user.profile_image, size: "80x80" %>
  <% else %>
    <%= image_tag 'no_image', size: "80x80" %>
  <% end %>
  <%= user.name %>
  <%= link_to "Show", user_path(user) %>
<% end %>